  "repository": {
    "type": "git",
    "url": "git@github.com:simon387/daocstarter.git"
  },
  "dependencies": {
    "datatables.net": "*",
    "datatables.net-dt": "*",
    "express": "*",
    "jquery": "*",
    "nedb": "^1.8.0",
    "node-powershell": "*"
  },
  "jshintConfig": {
    "esversion": "6"
